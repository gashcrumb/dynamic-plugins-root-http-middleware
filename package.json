{
  "name": "dynamic-plugins-example-root-http-middleware",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18 || 20"
  },
  "scripts": {
    "build": "backstage-cli repo build --all",
    "tsc": "tsc",
    "tsc:full": "tsc --skipLibCheck false --incremental false",
    "clean": "backstage-cli repo clean",
    "test": "backstage-cli repo test",
    "test:all": "backstage-cli repo test --coverage",
    "test:e2e": "playwright test",
    "fix": "backstage-cli repo fix",
    "lint": "backstage-cli repo lint --since origin/main",
    "lint:all": "backstage-cli repo lint",
    "prettier:check": "prettier --check .",
    "new": "backstage-cli new --scope internal",
    "export-local": "janus-cli package package-dynamic-plugins --force-export --export-to ${DYNAMIC_PLUGINS_ROOT:-./deploy}",
    "package-dynamic-plugins": "janus-cli package package-dynamic-plugins --force-export --tag ${PLUGIN_IMAGE_TAG:-localhost:5000/${USER}/example-root-http-middleware:latest}"
  },
  "workspaces": [
    "plugins/*"
  ],
  "devDependencies": {
    "@backstage/cli": "0.28.2",
    "@backstage/e2e-test-utils": "^0.1.1",
    "@janus-idp/cli": "^3.2.0",
    "@playwright/test": "^1.32.3",
    "@spotify/prettier-config": "^12.0.0",
    "concurrently": "^8.0.0",
    "lerna": "^7.3.0",
    "node-gyp": "^9.0.0",
    "prettier": "^2.3.2",
    "typescript": "5.6.2"
  },
  "resolutions": {
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "prettier": "@spotify/prettier-config",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.8.3"
}
